@{
    ViewData["Title"] = "Alta Cadete";
}

<hr />

@using (Html.BeginForm("altaCadete", "Home"))
{
    <table>
        <tr>
            <td>
                Nombre: 
            </td>
            <td>
                <label for="direccion">
                    <input type="text" name="nombre" id="nombre">
                </label>
            </td>
        </tr>
        <tr>
            <td>
                Direccion: 
            </td>
            <td>
                <label for="direccion">
                    <input type="text" name="direccion" id="direccion">
                </label>
            </td>
        </tr>
        <tr>
            <td>
                Telefono: 
            </td>
            <td>
                <label for="telefono">
                    <input type="text" name="telefono" id="telefono">
                </label>
            </td>
        </tr>
        <tr>
            <td>&nbsp</td>
            <td>&nbsp</td>
        </tr>
        <tr>
            <td>&nbsp</td>
            <td>
               <input type="submit", value="Alta">
            </td>
        </tr>
    </table>

<hr />

@Html.ActionLink("Cancelar", "Index", "Home")

}